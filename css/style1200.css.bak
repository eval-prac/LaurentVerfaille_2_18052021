h1 {
	font-size: 1.4em;
	line-height: 1.4em;
	font-weight: bolder;
}

h2 {
	padding-top: 1em;
	padding-bottom: 0.1em;
	font-size: 1.0em;
	line-height: 1.0em;
	font-weight: bolder;
}
h3 {
	font-size: 0.7em;
	line-height: 0.9em;
	font-weight: bolder;
}

p, h3 {
	margin-top:0.8vw;
	margin-bottom:0.2vw;
}
.star {
	margin-top:0px;
	margin-bottom:0px;
}
p, li, .fa-star {
	font-size: 0.6em;
	line-height: 0.6em;
	font-style: normal;
	font-variant: normal;
	font-weight: 400;
}

article {
	box-shadow: 0.1em 0.3em 0.8em #E0E0E0;
}

/* Zones */
body, header, main, footer, .container {
    margin:0px;
	padding:0px;
	display:flex;
    flex-direction: column;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: stretch;
}

/* Group */
.group_result {
    
	display:flex;
    flex-direction: column;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: stretch;
}


/* containers block */
.container_titre_menu {
    margin:0;
	padding:0;
	height: 4em;
	display: block;
	float: left;
}
.container_search {
	margin:0;
	padding:0;
	align-items: stretch;
}
.container_populaire {
	flex-grow: 1;
    margin:0;
	padding:0.7em;
	border-radius: 5px;
	display: flex;
	flex-direction: column;
    align-items: stretch;
	background-color: #F2F2F2;
}
.container_hebergement {
	width: 70%;
    margin:0 1.3em 0 0;
	padding: 0.7em 0 2em 0;
	border-radius: 5px;
	display: flex;
	flex-direction: column;
    align-items: stretch;
	background-color: #F2F2F2;
}
.container_activite {
	width: 100%;
    margin:0;
	padding:0;
	border-radius: 5px;
	display: flex;
	flex-direction: column;
    align-items: stretch;
}
.container_footer {
    margin:0;
	padding:0;
	display:flex;
    flex-direction: column;
    align-items: stretch;
	background-color: #F2F2F2;
}

.area_row {
	display: flex;
	flex-direction: row;
	justify-content: stretch;
	align-items: stretch;
	padding:3%;
}
.area_column {
	display: flex;
	flex-direction: column;
	justify-content: baseline;
	align-items: stretch;
	padding:0.3em 3% 0.3em 3%;
}

/*********************************/
/*********************************/
.zone_menu {
    margin:0;
	padding:0%;
	display: block;
}
.menu_titre {
	width:70px;
    margin:0 42% 0 0;
	padding:3% 0 3% 3%;
	display: block;
	float: left;
	color: #0065FC;
}
.menu_titre img {
	width: 100%;
}
.menu_hebergement {
	width:10%;
    margin:0;
	padding:3%;
	display: block;
	float: left;
	border-top: solid 2px white;
	text-align: center;
	font-size: 0.9em;
	color: #0065FC;
	font-weight: bold;
}
.menu_activity {
	width:10%;
	margin:0;
	padding:3%;
	display: block;
	float: left;
	border-top: solid 2px white;
	text-align: center;
	font-size: 1em;
	font-weight: bold;
}
.menu_activity  a {
	color: black;
}

.menu_subscribe {
	width:10%;
    margin:0;
	padding:3%;
	display: block;
	float: left;
	border-top: solid 2px white;
	text-align: center;
	font-size: 1em;
	font-weight: bolder;
}
.menu_subscribe a {
	color: #0065FC;
}

div:hover.menu_hebergement, div:hover.menu_activity, div:hover.menu_subscribe {
	border-top: solid 2px #0065FC;
}
div:hover.menu_hebergement a, div:hover.menu_activity a, div:hover.menu_subscribe a {
	color: #0065FC;
}
/***********************/

.radio {
	display:flex;
	flex-wrap: wrap;
	flex-direction: row;
	justify-content: flex-start;
	align-items: center;
	flex-basis: auto;
}
.radio_envelope {
	margin:5px;
	height:50px;
	border:solid 2px #F2F2F2;
	border-left-style: hidden;
	border-radius:50px;
	display:flex;
	flex-wrap: wrap;
	flex-direction: row;
	justify-content: flex-start;
	align-items: center;
	flex-basis: auto;
}
.radio_envelope:hover {
	color: #0065FC;
	box-shadow:5px 8px 5px #E0E0E0;
}
.radio_logo {
	width:50px;
	height:50px;
	border-radius:50%;
	background-color:#DEEBFF;
	display:flex;
	flex-wrap: wrap;
	flex-direction: row;
	justify-content: center;
	align-items: center;
}
.radio_text {
	padding:0 10px 0 10px;
	height: 50px;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 1.1em;
	line-height: 1.1em;
	font-weight: bold;
}
.radio_space {
	display: none;
}

.search_envelope {
	width:50%;
}

/***************************************/

.populaire_hebergement {
	padding-left: 3%;
	padding-right: 3%;
	display: flex;
	flex-direction: row-reverse;
	justify-content: stretch;
	align-items: stretch;
}



.container_populaire__div_article {
	display: flex;
	flex-direction: column;
	justify-content: baseline;
	align-items: stretch;
}
.container_hebergement__div_article {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
	justify-content: space-between;
    align-items: stretch;
}
.container_activite__div_article {
	height: 37vw;
	display: flex;
	flex-direction: column;
	flex-wrap: wrap;
	justify-content: space-between;
	align-content: space-between;
}

.container_populaire article {
	margin: 0.3em 0.5em 0.6em 0.5em;
	display:flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items: stretch;
	border-width:0px;
	border-radius:5px;
	padding:3px;
	background-color: #FFFFFF;
}
.container_hebergement article {
	width: 28%;
	margin: 0 0.5em 0.7em 0.5em;
	display:flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: stretch;
	border-width:0px;
	border-radius:5px;
	padding:3px;
	background-color: #FFFFFF;	
}
.container_activite article {
	width: 23%;
	margin-bottom: 1.5em;
	display:flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: stretch;
	border-width:0px;
	border-radius:5px;
	padding:3px;
	background-color: #FFFFFF;
}



.container_populaire div.figure {
	width: auto;
	height: auto;
}
.container_populaire div.figure img {
	display: block;
	width: 10vw;
	height: 10vw;
	border-top-left-radius:5px;
	border-bottom-left-radius:5px;
}
.container_hebergement div.figure {
	width: auto;
	height: auto;
}
.container_hebergement div.figure img {
	display: block;
	width: 100%;
	height: 8vw;
	border-top-left-radius:5px;
	border-top-right-radius:5px;
}
.container_activite div.figure {
	width: auto;
	height: 100%;
	overflow: auto;
}
.container_activite div.figure img {
	display: block;
	width: 100%;
	height: 100%;
	border-top-left-radius:5px;
	border-top-right-radius:5px;
}


.container_populaire .figcaption {
	flex-grow: 1;
    margin:0.7vw;
	display: flex;
	flex-wrap: wrap;
	flex-direction: column;
	justify-content: space-between;
	align-items: baseline;
}
.container_hebergement .figcaption {
	width: auto;
	flex-grow: 1;
    margin:0.7vw;
	display: flex;
	flex-wrap: wrap;
	flex-direction: column;
	justify-content: space-between;
	align-items: baseline;
}
.container_activite .figcaption {
	width: auto;
	height: 3vw;
    margin:0.7vw;
	display: flex;
	flex-wrap: wrap;
	flex-direction: column;
	justify-content: space-between;
	align-items: baseline;
}


.container_populaire .figcaption div:nth-child(1) {
	display: flex;
	flex-direction: column;
}
.container_hebergement .figcaption div:nth-child(1) {
	display: flex;
	flex-direction: column;
}
.container_activite .figcaption div:nth-child(1) {
	display: flex;
	flex-direction: column;
}

.container_populaire .figcaption div:nth-child(2) {
	display: flex;
	padding-top: 0.6em;
	font-size: 0.8em;
	color: #0065FC;
}
.container_hebergement .figcaption div:nth-child(2) {
	display: flex;
	padding-top: 0.6em;
	font-size: 0.8em;
	color: #0065FC;
}
.container_activite .figcaption div:nth-child(2) {
	display: flex;
	padding-top: 0.6em;
	font-size: 0.8em;
	color: #0065FC;
}


.container_activite article:nth-child(1) {
	height:100%;
}
.container_activite article:nth-child(4) {
	height:100%;
}

.container_footer ul {
	margin:0px;
	display: flex;
	flex-direction: column;
	flex-wrap: wrap;
	justify-content: flex-start;
	align-items: baseline;
	padding-left: 0px;
}
.container_footer li {
	margin: 0.8em 0px 0.8em 0px;
    list-style-type: none;
    list-style-position: inside;
}

.container_footer section {
	flex-grow: 1;
}

.area_footer {
	display: flex;
	flex-direction: row;
	justify-content: baseline;
	align-items: stretch;
	padding: 1em 3% 1.5em 3%;	
}


.container_activite .figcaption div:nth-child(1) p {
	display: none;
}
.container_activite .figcaption div:nth-child(2) {
	display: none;
}
