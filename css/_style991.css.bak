.container_activite article:nth-child(1) {
	height:100%;
}
.container_activite article:nth-child(1) div.figure {
	flex-grow: 1;
}
.container_activite article:nth-child(1) div.figcaption {
	flex-grow: 0;
}
.container_activite article:nth-child(4) {
	height:100%;
}
.container_activite article:nth-child(4) div.figure {
	flex-grow: 1;
}
.container_activite article:nth-child(4) div.figcaption {
	flex-grow: 0;
}

.container_footer ul {
	margin:0px;
	display: flex;
	flex-direction: column;
	flex-wrap: wrap;
	justify-content: flex-start;
	align-items: baseline;
	padding-left: 0px;
}
.container_footer li {
	margin: 0.5em 0px 0.5em 0px;
    list-style-type: none;
    list-style-position: inside;
}

.container_footer section {
	flex-grow: 1;
}

.area_footer {
	display: flex;
	flex-direction: row;
	justify-content: baseline;
	align-items: stretch;
	padding: 0.2em 3% 0.2em 3%;	
}

